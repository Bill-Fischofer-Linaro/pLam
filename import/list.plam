:import std

pair = \x y p. p x y
fst = \p. p T
snd = \p. p F
nil = F
isnil = \l.l (\a b.F) T
append = \h t f x.f h (t f x)
head = \l. l T T
tail = \l.fst (l (\a b. pair (snd b) (append a (snd b))) nil)

L0 = nil
L1 = append 1 L0
L2 = append 2 L1
L3 = append 3 L2
